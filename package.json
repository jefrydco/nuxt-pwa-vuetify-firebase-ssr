{
  "name": "nuxt-pwa-vuetify-firebase-ssr-starter",
  "version": "1.0.0",
  "private": true,
  "description": "Nuxt.js + PWA + Vuetify.js + Firebase starter project",
  "keywords": [
    "nuxt",
    "nuxt pwa",
    "nuxt.js",
    "nuxtjs",
    "vue",
    "vue pwa",
    "vue ssr",
    "vue universal",
    "vue.js",
    "vuejs",
    "vuetify",
    "vuetify.js",
    "vuetifyjs",
    "firebase"
  ],
  "homepage": "https://github.com/jefrydco/nuxt-pwa-vuetify-firebase-ssr#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jefrydco/nuxt-pwa-vuetify-firebase-ssr"
  },
  "license": "MIT",
  "author": {
    "name": "Jefry Dewangga",
    "url": "https://jefrydco.id"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "yarn test:unit",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:unit": "jest",
    "coverage": "codecov",
    "build:firebase": "yarn clean && yarn build && yarn copy && cd \"functions\" && yarn",
    "clean": "yarn clean:public && yarn clean:functions && yarn clean:static",
    "clean:functions": "rimraf \"functions/node_modules\" && rimraf \"functions/.nuxt\"",
    "clean:public": "rimraf \"public/**/*.*!(md)\" && rimraf \"public/_nuxt\"",
    "clean:static": "rimraf \"src/static/sw.js\"",
    "copy": "cpx \"functions/.nuxt/dist/*\" \"public/_nuxt\" && cpx \"src/static/**/*.*!(md)\" \"public\"",
    "deploy": "firebase deploy --only functions,hosting",
    "start:firebase": "firebase serve --only functions,hosting"
  },
  "dependencies": {
    "@nuxt/http": "^0.3.8",
    "@nuxtjs/google-analytics": "^2.2.2",
    "@nuxtjs/pwa": "^3.0.0-beta.19",
    "@nuxtjs/sentry": "^3.0.1",
    "@nuxtjs/sitemap": "^2.0.1",
    "lru-cache": "^5.1.1",
    "nuxt": "^2.11.0",
    "vee-validate": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/vuetify": "^1.9.0",
    "@vue/test-utils": "^1.0.0-beta.30",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "codecov": "^3.6.1",
    "cpx": "^1.5.0",
    "cypress": "^3.8.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.2.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-nuxt": "^0.5.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.1.2",
    "eslint-plugin-vuetify": "^1.0.0-beta.5",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "vue-jest": "^3.0.5"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.4.1",
    "yarn": ">=1.12.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
